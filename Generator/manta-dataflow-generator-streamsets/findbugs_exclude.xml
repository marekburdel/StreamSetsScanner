<FindBugsFilter>

    <Match>
        <Priority value="2"/> <!-- mid -->
        <Bug category="EXPERIMENTAL, I18N"/>
    </Match>

    <Match>
        <Or>
            <Priority value="2"/> <!-- mid -->
            <Priority value="3"/> <!-- low -->
        </Or>
        <Or>
            <Bug pattern="BC_UNCONFIRMED_CAST, MS_PKGPROTECT, UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
            <Bug code="EI, EI2, IS, Se"/>
        </Or>
    </Match>

    <!-- Filter out derived parser and lexer -->
    <Match>
        <Class name="~eu\.profinit\.manta\.dataflow\.generator\.streamsets\.analyzer\.stage\.field\.parser\.(?!Abstract).*" />
    </Match>

</FindBugsFilter>